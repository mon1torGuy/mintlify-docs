{
	"$schema": "https://mintlify.com/schema.json",
	"name": "typeauth Docs",
	"logo": {
		"dark": "/logo/dark.svg",
		"light": "/logo/light.svg"
	},
	"openapi": ["openapi.json"],

	"favicon": "/favicon.svg",
	"colors": {
		"primary": "#35958b",
		"light": "#07C983",
		"dark": "#35958b",
		"anchors": {
			"from": "#0D9373",
			"to": "#07C983"
		}
	},
	"topbarLinks": [
		{
			"name": "Support",
			"url": "mailto:support@typeauth.com"
		}
	],
	"topbarCtaButton": {
		"name": "Dashboard",
		"url": "https://app.typeauth.com"
	},
	"tabs": [
		{
			"name": "API Reference",
			"url": "api-reference",
			"openapi": "/openapi.json"
		}
	],
	"anchors": [],
	"navigation": [
		{
			"group": "Get Started",
			"pages": ["introduction", "reference", "SDK", "boilerplates"]
		},
		{
			"group": "Features",
			"pages": [
				"Abuse and Fraud Prevention",
				"LLM Caching",
				"API Caching",
				"Rate Limiting",
				"Authentication"
			]
		},

		{
			"group": "API Documentation",

			"pages": ["api-reference/introduction"]
		},
		{
			"group": "Application",
			"pages": [
				"api-reference/applications/create-a-new-application",
				"api-reference/applications/delete-an-application",
				"api-reference/applications/list-all-applications",
				"api-reference/applications/retrieve-an-application-by-id",
				"api-reference/applications/update-an-application"
			]
		},
		{
			"group": "Keys / Tokens",
			"pages": [
				"api-reference/keys/create-a-key",
				"api-reference/keys/list-all-keys",
				"api-reference/keys/retrieve-keys-by-appid",
				"api-reference/keys/update-a-key",
				"api-reference/keys/update-a-keys-remaining-limit",
				"api-reference/keys/delete-a-key",
				"api-reference/keys/verify-authentication-key"
			]
		},
		{
			"group": "JSON Web Tokens",
			"pages": [
				"api-reference/jwts/create-jwt",
				"api-reference/jwts/delete-jwt",
				"api-reference/jwts/list-jwts"
			]
		},
		{
			"group": "Monitors",
			"pages": [
				"api-reference/monitors/create-a-monitor",
				"api-reference/monitors/delete-monitor",
				"api-reference/monitors/get-monitors",
				"api-reference/monitors/update-monitor"
			]
		},
		{
			"group": "Feedback",
			"pages": [
				"api-reference/feedback/create-feedback",
				"api-reference/feedback/get-feedback"
			]
		}
	],
	"feedback": {
		"thumbsRating": true
	},
	"modeToggle": {
		"default": "dark",
		"isHidden": true
	},
	"footerSocials": {
		"github": "https://github.com/typeauth",
		"linkedin": "https://www.linkedin.com/company/typeauth"
	}
